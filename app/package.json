{
  "name": "biznesinfo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "sync:biznesinfo": "node scripts/sync_biznesinfo_data.mjs",
    "keywords:gen": "npx tsx scripts/generate_company_keywords.ts",
    "index:meili": "npx tsx scripts/index_meilisearch.ts",
    "qa:scenarios:core": "node scripts/build_ai_request_qa_scenarios.mjs",
    "qa:scenarios:external": "node scripts/build_external_ai_request_scenarios.mjs",
    "qa:external:docs": "node scripts/build_external_challenge_docs.mjs",
    "qa:external:minimax:50": "node scripts/generate_minimax_50_external_challenges.mjs",
    "qa:scenarios:chatgpt-pro": "node scripts/build_chatgpt_pro_test_pack.mjs",
    "qa:scenarios:multi-model": "npm run qa:scenarios:chatgpt-pro && npm run qa:scenarios:external && node scripts/build_multi_model_test_pack.mjs && npm run qa:external:docs",
    "qa:run": "node scripts/ai-request-qa-runner.mjs",
    "qa:run:links": "node scripts/ai-request-qa-runner.mjs --scenarios qa/ai-request/scenarios.regressions.link-integrity.json --prepare-paid --paid-limit 1500",
    "qa:run:geo-ambiguity": "node scripts/ai-request-qa-runner.mjs --scenarios qa/ai-request/scenarios.regressions.geo-ambiguity.json --prepare-paid --paid-limit 1500",
    "qa:run:multi-step": "node scripts/ai-request-qa-runner.mjs --scenarios qa/ai-request/scenarios.regressions.multi-step-journeys.json --prepare-paid --paid-limit 2200",
    "qa:run:user-ideas": "node scripts/ai-request-qa-runner.mjs --scenarios qa/ai-request/scenarios.regressions.user-ideas-multistep-variants.json --prepare-paid --paid-limit 2600",
    "qa:run:user-ideas:beet": "node scripts/ai-request-qa-runner.mjs --scenarios qa/ai-request/scenarios.regressions.user-ideas-multistep-variants.json --prepare-paid --paid-limit 2600 --tag beet",
    "qa:run:user-ideas:website-scan": "node scripts/ai-request-qa-runner.mjs --scenarios qa/ai-request/scenarios.regressions.user-ideas-multistep-variants.json --prepare-paid --paid-limit 2600 --tag website_scan",
    "qa:run:user-ideas:website-scan:deep": "node scripts/ai-request-qa-runner.mjs --scenarios qa/ai-request/scenarios.regressions.user-ideas-multistep-variants.json --prepare-paid --paid-limit 2600 --tag website_scan_deep",
    "qa:trend:geo-ambiguity": "node scripts/track_geo_ambiguity_pass_rate_trend.mjs --require-target-reached",
    "qa:trend:geo-ambiguity:strict": "node scripts/track_geo_ambiguity_pass_rate_trend.mjs --require-target-reached --max-last-green-age-hours 12",
    "qa:trend:geo-ambiguity:ci": "node scripts/track_geo_ambiguity_pass_rate_trend.mjs --require-target-reached --max-last-green-age-hours 12 --emit-ci-summary-only",
    "qa:trend:geo-ambiguity:ci-fast": "node scripts/track_geo_ambiguity_pass_rate_trend.mjs --require-target-reached --max-last-green-age-hours 12 --emit-ci-summary-only --no-write",
    "qa:trend:geo-ambiguity:ci:freshness-first": "node scripts/track_geo_ambiguity_pass_rate_trend.mjs --require-target-reached --max-last-green-age-hours 12 --emit-ci-summary-only --no-write --gate-order freshness-first",
    "qa:run:dirty": "node scripts/ai-request-qa-runner.mjs --scenarios qa/ai-request/scenarios.dirty.realworld.json --prepare-paid --paid-limit 1200",
    "qa:run:chatgpt-pro": "npm run qa:scenarios:chatgpt-pro && node scripts/ai-request-qa-runner.mjs --scenarios qa/ai-request/scenarios.chatgpt-pro.master.json --prepare-paid --paid-limit 2500",
    "qa:run:external": "npm run qa:scenarios:external && node scripts/ai-request-qa-runner.mjs --scenarios qa/ai-request/external-challenges/scenarios.normalized.json --prepare-paid --paid-limit 2500",
    "qa:run:multi-model": "npm run qa:scenarios:multi-model && node scripts/ai-request-qa-runner.mjs --scenarios qa/ai-request/scenarios.multi-model.master.json --prepare-paid --paid-limit 4000",
    "qa:judge": "node scripts/rate_ai_usefulness_with_judges.mjs --judges gemini,kimi --min-user-satisfaction 0.72 --min-continue-rate 0.72 --max-generic-fallback-rate 0.35",
    "qa:judge:triad": "node scripts/rate_ai_usefulness_with_judges.mjs --judges gemini,kimi,minimax --min-user-satisfaction 0.72 --min-continue-rate 0.72 --max-generic-fallback-rate 0.35",
    "qa:judge:dual": "node scripts/rate_ai_usefulness_with_judges.mjs --judges kimi,minimax --min-user-satisfaction 0.72 --min-continue-rate 0.72 --max-generic-fallback-rate 0.35",
    "qa:advise": "node scripts/advise_ai_with_judges.mjs --advisors gemini,kimi",
    "qa:advise:triad": "node scripts/advise_ai_with_judges.mjs --advisors gemini,kimi,minimax",
    "qa:advise:dual": "node scripts/advise_ai_with_judges.mjs --advisors kimi,minimax",
    "qa:cycle:extended:triad": "node scripts/run_geo_ambiguity_long_cycle.mjs --mode triad --scope extended",
    "qa:cycle:extended:dual": "node scripts/run_geo_ambiguity_long_cycle.mjs --mode dual --scope extended",
    "qa:cycle:geo-ambiguity:triad": "node scripts/run_geo_ambiguity_long_cycle.mjs --mode triad --scope geo",
    "qa:cycle:geo-ambiguity:dual": "node scripts/run_geo_ambiguity_long_cycle.mjs --mode dual --scope geo",
    "qa:premerge:geo-ambiguity": "npm run qa:run:geo-ambiguity && npm run qa:trend:geo-ambiguity",
    "qa:cycle:user-ideas:triad": "npm run qa:run:user-ideas && npm run qa:judge:triad && npm run qa:advise:triad",
    "qa:cycle:user-ideas:dual": "npm run qa:run:user-ideas && npm run qa:judge:dual && npm run qa:advise:dual",
    "qa:cycle:user-ideas:always:triad": "node scripts/run_geo_ambiguity_long_cycle.mjs --mode triad --scope user-ideas",
    "qa:cycle:user-ideas:always:dual": "node scripts/run_geo_ambiguity_long_cycle.mjs --mode dual --scope user-ideas",
    "qa:cycle:user-ideas:always:plan": "node scripts/run_geo_ambiguity_long_cycle.mjs --mode dual --scope user-ideas --dry-run",
    "qa:cycle:multi-step:triad": "npm run qa:run:multi-step && npm run qa:judge:triad && npm run qa:advise:triad",
    "qa:cycle:multi-step:dual": "npm run qa:run:multi-step && npm run qa:judge:dual && npm run qa:advise:dual"
  },
  "dependencies": {
    "meilisearch": "^0.48.2",
    "next": "16.1.1",
    "nodemailer": "^6.9.15",
    "pg": "^8.13.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.9",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
